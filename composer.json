{
    "name": "patkruk/laravel-cached-settings",
    "description": "A simple cached config container for Laravel 4",
    "version": "1.0.6",
    "license": "MIT",
    "keywords": ["laravel", "config container", "cached configs", "cached settings"],
    "authors": [
        {
            "name": "Patryk Kruk",
            "email": "patkruk@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*@dev",
        "illuminate/cache": "4.*@dev"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Patkruk\\LaravelCachedSettings\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
